{
  "name": "@aotoki/slidev-theme-terraforming",
  "version": "0.3.1",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^51.6.0"
  },
  "devDependencies": {
    "@slidev/cli": "^51.6.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "light",
    "defaults": {
      "aspectRatio": "16/9",
      "transition": "view-transition",
      "fonts": {
        "sans": "Futura",
        "mono": "Fira Code"
      }
    }
  }
}
